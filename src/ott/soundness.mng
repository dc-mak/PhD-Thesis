    \step{<2>4}{$[[ cdot ; L0 ; N ; nR'  |- Owned < Uninit , ct > ( inj ptr ) <= pt ]]$ by
        \ResSynPred.}

    \step{<2>5}{Prove typing with \SpineRet; \stepref{<2>4} -- \stepref{<2>1} with
        \SpineRes, \SpineLog, \SpinePhi, \SpineComp{}
        respectively; \StmtRetT{}.}

