    \step{<2>2}{$[[ smt ( N => inj pval = inj pval ) ]]$ trivially.}

    \step{<2>3}{$[[ cdot ; L0 ; N ; nR' |- Owned < Init , ct > ( inj ptr ) => pt ]]$, already established.}

    \step{<2>4}{Prove typing with \SpineRet; \stepref{<2>3} -- \stepref{<2>1} with
