\step{<1>12}{\case{%
    \ActionKillStatic.
}{%

    \assume{%
        $[[ cdot ; L0 ; N ; nR |- res_term | kill < ct > ( ptr ) => sigma _ : unit . I ]]$
        and $[[ alloct | < h + f ; res_term > ||v < heap + { pt & None } + { Alloc ( ptrt ) ( oarg ) & None } ; < Owned < _init , ct > ( inj ptr ) , Alloc ( ptrt ) > > ]]$.\\[\baselineskip]}

